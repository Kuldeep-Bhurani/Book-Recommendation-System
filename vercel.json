{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
  "configurations": [
    {
      "env": {
        "SKLEARN_ALLOW_DEPRECATED_SKLEARN_PACKAGE_INSTALL" = true
      }
    }
  ]
}
